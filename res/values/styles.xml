<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@android:style/Theme" />
    <style name="Theme.GalleryItem" parent="@style/Theme" />
    <style name="ActionBar">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/actionbar_background</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/actionbar_height</item>
    </style>
    <style name="ActionBarItem">
        <item name="android:padding">5.0dip</item>
        <item name="android:layout_width">@dimen/actionbar_item_width</item>
        <item name="android:layout_height">@dimen/actionbar_height</item>
        <item name="android:layout_marginLeft">1.0px</item>
    </style>
    <style name="ActionBarHomeItem" parent="@style/ActionBarItem">
        <item name="android:layout_marginLeft">0.0px</item>
        <item name="android:layout_marginRight">1.0px</item>
    </style>
    <style name="ActionBarHomeLogo">
        <item name="android:layout_marginLeft">0.0px</item>
        <item name="android:layout_marginRight">1.0px</item>
    </style>
    <style name="ActionBarProgressBar" parent="@android:style/Widget.ProgressBar.Small" />
    <style name="application">
        <item name="android:background">#ffffffff</item>
    </style>
    <style name="textRoot" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="staticText" parent="@style/textRoot">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="gatewayTitle" parent="@style/textRoot">
        <item name="android:textSize">20.0sp</item>
    </style>
    <style name="gatewayText" parent="@style/textRoot">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="itemTitleText" parent="@style/textRoot">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="itemPriceText" parent="@style/textRoot">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="moreInfoText" parent="@style/textRoot">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="itemRatingText" parent="@style/textRoot">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffaaaaaa</item>
    </style>
    <style name="orderHeader" parent="@style/textRoot">
        <item name="android:textSize">12.0sp</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">3.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>
    <style name="orderHeaderTotal" parent="@style/textRoot">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">3.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>
    <style name="slotSelectTableText" parent="@style/textRoot">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="slotSelectTableTitle" parent="@style/textRoot">
        <item name="android:textSize">12.0sp</item>
    </style>
    <style name="SummaryPageReceipt">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">3.0dip</item>
    </style>
    <style name="DashboardButton">
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">@color/menu_title</item>
        <item name="android:gravity">left</item>
        <item name="android:background">@drawable/btn_gateway</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:paddingRight">20.0dip</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">36.0dip</item>
        <item name="android:layout_marginLeft">6.0dip</item>
        <item name="android:layout_marginRight">6.0dip</item>
        <item name="android:layout_marginBottom">3.0dip</item>
    </style>
    <style name="itemTitleText2" parent="@style/textRoot">
        <item name="android:textSize">14.0sp</item>
    </style>
    <style name="cartQty" parent="@style/textRoot">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="itemPriceText2" parent="@style/textRoot">
        <item name="android:textSize">12.0sp</item>
        <item name="android:paddingRight">3.0sp</item>
    </style>
    <style name="cartRow" parent="@style/textRoot">
        <item name="android:textSize">10.0sp</item>
    </style>
    <style name="detailHeader" parent="@style/textRoot">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff669900</item>
        <item name="android:paddingRight">3.0sp</item>
    </style>
    <style name="detailContents" parent="@style/textRoot">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="emptyCart" parent="@style/textRoot">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="Animations" />
    <style name="Animations.PopDownMenu" parent="@style/Animations" />
    <style name="Animations.PopDownMenu.Left" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
    </style>
    <style name="Animations.PopDownMenu.Right" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
    </style>
    <style name="Animations.PopDownMenu.Center" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_top</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottom</item>
    </style>
    <style name="Animations.PopUpMenu" parent="@style/Animations" />
    <style name="Animations.PopUpMenu.Left" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topright_to_bottomleft</item>
    </style>
    <style name="Animations.PopUpMenu.Right" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topleft_to_bottomright</item>
    </style>
    <style name="Animations.PopUpMenu.Center" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_top</item>
    </style>
    <style name="NoTitleBar" parent="@android:style/Theme.NoTitleBar" />
</resources>
